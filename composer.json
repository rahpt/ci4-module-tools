{
    "name": "rahpt/ci4-module-tools",
    "description": "Development tools and module marketplace for CodeIgniter 4 module system",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "codeigniter",
        "codeigniter4",
        "modules",
        "development",
        "tools",
        "generator",
        "marketplace",
        "rahpt"
    ],
    "homepage": "https://github.com/rahpt/ci4-module-tools",
    "support": {
        "issues": "https://github.com/rahpt/ci4-module-tools/issues",
        "source": "https://github.com/rahpt/ci4-module-tools",
        "docs": "https://github.com/rahpt/ci4-module-tools#readme"
    },
    "authors": [
        {
            "name": "Rahpt",
            "email": "ci4-module@rah.pt",
            "homepage": "https://github.com/rahpt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.5",
        "rahpt/ci4-module": "*",
        "rahpt/ci4-module-nav": "*",
        "codeigniter4/settings": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "codeigniter4/devkit": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Rahpt\\Ci4ModuleTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rahpt\\Ci4ModuleTools\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html build/coverage"
    },
    "extra": {
        "codeigniter4": {
            "discover": [
                "Rahpt\\Ci4ModuleTools\\Config\\Registrar"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}